{
      // Use IntelliSense to learn about possible attributes.
      // Hover to view descriptions of existing attributes.
      // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
      "version": "0.2.0",
      "configurations": [

      


            {
                  "name": "debug React in VS Code",
                  "type": "chrome",
                  "request": "launch",
                  "url": "http://localhost:3000",
                  "webRoot": "${workspaceFolder}/src",
                  "sourceMapPathOverrides": {
                        "webpack:///src/*": "${webRoot}/*"
                        }
            },
            {
                  "name": "Generate SVG's Python",
                  "type": "python",
                  "request": "launch",
                  "program": "${file}",
                  "console": "integratedTerminal"
            },
            {
                  "type": "node",
                  "request": "launch",
                  "name": "Generate Store",
                  "skipFiles": [
                        "<node_internals>/**"
                  ],
                  "program": "${workspaceFolder}/script/generateStore.js"
            },
            {
                  "type": "node",
                  "request": "launch",
                  "name": "Generate Location Data",
                  "skipFiles": [
                        "<node_internals>/**"
                  ],
                  "program": "${workspaceFolder}/script/generateLocationData.js"
            },
            {
                  "type": "node",
                  "request": "launch",
                  "name": "Generate Lunr Data",
                  "skipFiles": [
                        "<node_internals>/**"
                  ],
                  "program": "${workspaceFolder}/script/generateAllWitnessLunrData.js"
            },
            {
                  "type": "node",
                  "request": "launch",
                  "name": "Generate Date Info",
                  "skipFiles": [
                        "<node_internals>/**"
                  ],
                  "program": "${workspaceFolder}/script/generateDates.js"
            },
            {
                  "type": "node",
                  "request": "launch",
                  "name": "Generate Manuscript HTML",
                  "skipFiles": [
                        "<node_internals>/**"
                  ],
                  "program": "${workspaceFolder}/script/generateManuscriptHtml.js"
            },
          
      ]
}